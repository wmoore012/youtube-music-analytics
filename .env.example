# Database Configuration
DB_HOST=localhost
DB_PORT=3306
DB_USER=your_db_user
DB_PASS=your_db_password
DB_NAME=youtube_analytics
DB_NAME=youtube_analytics

# YouTube API Configuration
YOUTUBE_API_KEY=your_youtube_api_key_here

# Artist color palette mapping for charts (JSON object)
# Example: {"Artist A": "#1f77b4", "Artist B": "#ff7f0e"}
ARTIST_COLORS_JSON=
ARTIST_COLORS_FILE=

# Revenue assumptions
# Default RPM ($ per 1,000 views)
REVENUE_RPM_DEFAULT=3.0
# Optional per-artist RPM overrides as JSON
# Example: {"Artist A": 4.5, "Artist B": 2.8}
REVENUE_RPM_MAP_JSON=

# YouTube Data Retention Compliance
# IMPORTANT: YouTube API Terms of Service require data retention limits
# Academic/Research use: 30 days recommended
# Commercial use: Check current YouTube API ToS for requirements
# Educational use: 180 days maximum recommended
YOUTUBE_DATA_RETENTION_DAYS=30

# Comment and User Data Retention
# For user-generated content (comments), shorter retention is recommended
YOUTUBE_COMMENT_RETENTION_DAYS=30

# ETL Configuration
YT_FETCH_COMMENTS=1
YT_COMMENTS_PER_VIDEO=80

# ETL Pipeline Settings
ETL_BATCH_SIZE=50
ETL_ENABLE_CACHING=true
ETL_LOG_LEVEL=INFO

# Sentiment Analysis Configuration
SENTIMENT_USE_ML_MODELS=false
SENTIMENT_VADER_WEIGHT=0.7
SENTIMENT_ML_WEIGHT=0.3
SENTIMENT_ENABLE_SARCASM_DETECTION=true
SENTIMENT_MUSIC_CONTEXT_BOOST=1.2
SENTIMENT_ENABLE_CUSTOM_LEXICONS=true

# Development Mode Settings
DEVELOPMENT_MODE=false
MAX_SONGS_DEV=10
MAX_TWEETS_DEV=10

# YouTube Channel URLs (Configure with your target channels)
# Example format:
# YT_CHANNEL_1=https://www.youtube.com/@your_channel_1
# YT_CHANNEL_2=https://www.youtube.com/@your_channel_2
# YT_CHANNEL_3=https://www.youtube.com/@your_channel_3
# YT_CHANNEL_4=https://www.youtube.com/@your_channel_4
# YT_CHANNEL_5=https://www.youtube.com/@your_channel_5
